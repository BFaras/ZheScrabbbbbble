<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TestChatBox" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@color/beige</item>
        <item name="backgroundColor">@color/beige</item>
        <item name="android:textColorHighlight">@color/beige</item>
        <item name="android:textColor">@color/darkForestGreen</item>
        <item name="colorPrimary">@color/corail</item>
        <item name="colorPrimaryVariant">@color/darkCorail</item>
        <item name="colorOnPrimary">@color/lightBeige</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#bccca9</item>
        <item name="colorSecondaryVariant">@color/Brown</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="doubleWord">@color/lightCorail</item>
        <item name="tripleWord">@color/forestGreen</item>
        <item name="doubleLetter">@color/lightForest</item>
        <item name="grid">@color/black</item>
        <item name="coords">@color/black</item>
        <item name="playerSelection">@color/teal_200</item>
        <item name="boardBackground">@color/lightBeige</item>
        <item name="lettersOnBoard">@color/NavajoWhite</item>
        <item name="lettersInHand">@color/AntiqueWhite</item>
        <item name="chatBox">@color/lightBeige</item>
        <item name="activeTile">@color/purple_700</item>
        <item name="backgroundProfile">@color/lightBeige</item>
        <item name="chatIconNotif">#38683A</item>
        <item name="chatCanalNotif">#EEE6A3</item>
    </style>

    <style name="Theme.TestChatBox.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="PinkTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
