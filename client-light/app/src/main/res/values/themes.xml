        <item name="android:textColorHighlight">#e0cddc</item>
        <item name="android:textColor">@color/black</item>
        <item name="colorPrimary">#fffbfb</item>
        <item name="colorPrimaryVariant">#e0cddc</item>
        <item name="colorOnPrimary">#7e0062</item>
        <item name="colorSecondary">#cb4f7f</item>
        <item name="colorSecondaryVariant">@color/MediumVioletRed</item>
        <item name="android:statusBarColor">#cb4f7f</item>
        <item name="playerSelection">@color/BlueViolet</item>
        <item name="colorOnSecondary">@color/DarkSunset</item>
        <item name="doubleWord">@color/lightCorail</item>
        <item name="tripleWord">@color/IndianRed</item>
        <item name="doubleLetter">@color/RosyBrown</item>
        <item name="grid">@color/black</item>
        <item name="coords">@color/PurpleSunset</item>
        <item name="boardBackground">@color/lightBeige</item>
        <item name="lettersOnBoard">@color/BabyPink</item>
        <item name="lettersInHand">@color/AntiqueWhite</item>
        <item name="chatBox">@color/white</item>
        <item name="activeTile">@color/purple_700</item>
        <item name="backgroundProfile">@color/LightBabyPink</item>
        <item name="chatIconNotif">#f7dfed</item>
        <item name="chatCanalNotif">#DFB2CC</item>
    </style>

